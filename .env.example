# MinIO credentials (example values, change in actual .env)
MINIO_ACCESS_KEY=your_minio_access_key
MINIO_SECRET_KEY=your_minio_secret_key

# Allowed origins (adjust for security)
ALLOWED_INSIDE="http://localhost:80"
ALLOWED_OUTSIDE="http://localhost:3000"

# Frontend port mapping
INSIDE_FRONTEND_PORT=80
OUTSIDE_FRONTEND_PORT=3000

# Database credentials (example values, change in actual .env)
DB_NAME=your_database_name
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password